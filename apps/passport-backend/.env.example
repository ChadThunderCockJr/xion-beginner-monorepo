# Paywall Passport - Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=development

# Reclaim Protocol Credentials
# Get these from https://dev.reclaimprotocol.org
RECLAIM_APP_ID=your_reclaim_app_id_here
RECLAIM_APP_SECRET=your_reclaim_app_secret_here
RECLAIM_PROVIDER_ID=your_reclaim_provider_id_here

# URLs
API_BASE_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000

# Age Verification Settings
MIN_CREDIT_SCORE_FOR_21=600
